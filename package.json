{
  "name": "font-component",
  "version": "1.0.0",
  "description": "This is a small project I started in August 2022 and finished within the first week. I collected some fonts on https://www.dafont.com/ and converted them into webfonts.",
  "main": "index.js",
  "browserslist": "last 10 versions",
  "scripts": {
    "watchSass": "sass --watch sass/style.scss:css/style.css",
    "liveServer": "live-server",
    "prefixCSS": "postcss css/style.css --use autoprefixer -o css/style.prefix.css",
    "uglifyCSS": "uglifycss css/style.prefix.css --output _dist/css/style.css",
    "modifyCSS": "npm-run-all prefixCSS uglifyCSS",
    "startDev": "npm-run-all --parallel watchSass liveServer"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "uglifycss": "^0.0.29"
  }
}
